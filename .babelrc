{
  "presets": [
    [
      "env",
      { "modules": false, "loose": true }
    ],
    "react"
  ],
  "plugins": [
    "transform-object-rest-spread",
    [
      "transform-class-properties",
      { "loose": true }
    ],
    [
      "react-intl-auto",
      {
        "filebase": false
      }
    ]
  ]
}

